# Server Configuration
PORT=5000

# PostgreSQL Configuration (Sandbox Database)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=ciphersqlstudio
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_password

# MongoDB Configuration (Persistence Database)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/ciphersqlstudio?retryWrites=true&w=majority

# LLM API Configuration
# For OpenAI
OPENAI_API_KEY=your_openai_api_key

# For Google Gemini (alternative)
GEMINI_API_KEY=your_gemini_api_key

# LLM Provider (openai or gemini)
LLM_PROVIDER=openai

